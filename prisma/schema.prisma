datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Item1 {
  url       String  @id
  title     String
  price     Float
  thumbnail String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Item2 {
  id        String  @id @default(uuid())
  url       String  @unique
  title     String
  price     Float
  thumbnail String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

   @@index([url])
}
